#include<iostream>
using namespace std;
void findUnique(int *arr, int size)
{
    int ans=0;
    for(int i=0;i<size;i++){
        ans = ans ^ arr[i];
    }
    cout<< ans;
}

int main(){

    int n;
cout<<"The Unique element in the array is: "<<endl;

int array[9]={2,3,1,1,9, 6, 3, 6, 2};
findUnique(array, 9);
return 0;
}
