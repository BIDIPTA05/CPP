#include<iostream>
using namespace std;
int main(){

    int size,sum=0;
    int check[100];
    cout << "Enter the size of the array: "<<endl;
    cin >> size;
    //array inputs
    cout << "ENTER THE ELEMENTS OF THE ARRAY: ";
    for (int i = 0; i < size;i++){
        cin >> check[i];
        cout << endl;
    }

//adding array
for(int i = 0; i < size; i++){
    sum = sum+check[i];
}
//displaying sum
    cout << "The sum of the array is: "<<sum<<endl;

return 0;
}
